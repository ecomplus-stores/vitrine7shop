      "type": "custom-html",
      "html": "<!DOCTYPE html>\n<html>\n<head>\n  <style>\n    .slider-container {\n      width: 1200px;\n      overflow: hidden;\n      position: relative;\n      display: flex;\n      justify-content: space-between;\n      align-items: center;\n    }\n    \n    .slider-content {\n      display: flex;\n      transition: transform 0.3s ease-in-out;\n    }\n    \n    .slider-item {\n      flex: 0 0 150px;\n      height: 100px;\n      margin: 10px;\n      background-color: lightgray;\n      cursor: pointer;\n      position: relative;\n    }\n    \n    .slider-item img {\n      width: 100%;\n      height: 100%;\n      object-fit: cover;\n    }\n    \n    .slider-prev,\n    .slider-next {\n      position: absolute;\n      top: 50%;\n      transform: translateY(-50%);\n      background-color: rgba(0, 0, 0, 0.5);\n      color: white;\n      padding: 10px;\n      border: none;\n      cursor: pointer;\n      z-index: 1;\n    }\n    \n    .slider-prev {\n      left: 10px;\n    }\n    \n    .slider-next {\n      right: 10px;\n    }\n  </style>\n</head>\n<body>\n  <div class=\"slider-container\">\n    <button class=\"slider-prev\">&#8249;</button>\n    <div class=\"slider-content\">\n      <a href=\"https://vitrine7shop.com.br/#a-brinquedos-e-hobbies\" class=\"slider-item\">\n        <img src=\"template/public/img/uploads/vitrine7shop_categoria-binquedos-e-hobbies.webp\" alt=\"Imagem 1\">\n      </a>\n      <a href=\"https://vitrine7shop.com.br/#a-calcados-roupas-e-bolsas\" class=\"slider-item\">\n        <img src=\"template/public/img/uploads/vitrine7shop_categoria-calcados-roupas-e-bolsas.webp\" alt=\"Imagem 2\">\n      </a>\n      <a href=\"https://vitrine7shop.com.br/#a-casa-moveis-e-decoracao\" class=\"slider-item\">\n        <img src=\"template/public/img/uploads/vitrine7shop_categoria-casa-moveis-e-decoracao.webp\" alt=\"Imagem 3\">\n      </a>\n      <a href=\"https://vitrine7shop.com.br/#a-saude-e-bem-estar\" class=\"slider-item\">\n        <img src=\"template/public/img/uploads/vitrine7shop_categoria-saude-e-bem-estar.webp\" alt=\"Imagem 4\">\n      </a>\n      <a href=\"https://vitrine7shop.com.br/#a-escritorio-papelaria\" class=\"slider-item\">\n        <img src=\"template/public/img/uploads/vitrine7shop_categoria-escritorio-e-papelaria.webp\" alt=\"Imagem 5\">\n      </a>\n      <a href=\"https://vitrine7shop.com.br/#a-instrumentos-musicais\" class=\"slider-item\">\n        <img src=\"template/public/img/uploads/vitrine7shop_categoria-instrumentos-musicais.webp\" alt=\"Imagem 6\">\n      </a>\n      <a href=\"https://vitrine7shop.com.br/#a-games-e-consoles\" class=\"slider-item\">\n        <img src=\"template/public/img/uploads/vitrine7shop_categoria-games-e-consoles.webp\" alt=\"Imagem 7\">\n      </a>\n      <a href=\"https://vitrine7shop.com.br/#a-cameras-e-acessorios\" class=\"slider-item\">\n        <img src=\"template/public/img/uploads/vitrine7shop_categoria-camera-e-acessorios_.webp\" alt=\"Imagem 8\">\n      </a>\n      <a href=\"https://vitrine7shop.com.br/#a-eletronicos-audio-e-video\" class=\"slider-item\">\n        <img src=\"template/public/img/uploads/vitrine7shop_categoria-eletronicos-audio-e-video.webp\" alt=\"Imagem 9\">\n      </a>\n      <a href=\"https://vitrine7shop.com.br/#a-informatica-e-acessorios\" class=\"slider-item\">\n        <img src=\"template/public/img/uploads/vitrine7shop_categoria-informatica-e-acessorios.webp\" alt=\"Imagem 10\">\n      </a>\n    </div>\n    <button class=\"slider-next\">&#8250;</button>\n  </div>\n\n  <script>\n    const container = document.querySelector('.slider-container');\n    const content = document.querySelector('.slider-content');\n    const items = document.querySelectorAll('.slider-item');\n    const itemWidth = items[0].offsetWidth;\n    const prevButton = document.querySelector('.slider-prev');\n    const nextButton = document.querySelector('.slider-next');\n    let position = 0;\n    let intervalId = null;\n\n    function slide(direction) {\n      const slidesToShow = Math.floor(container.offsetWidth / itemWidth);\n      const minPosition = 0;\n      const maxPosition = (items.length - slidesToShow) * itemWidth;\n\n      if (direction === 'next' && position > -maxPosition) {\n        position -= itemWidth;\n      } else if (direction === 'prev' && position < minPosition) {\n        position += itemWidth;\n      } else if (direction === 'next' && position <= -maxPosition) {\n        position = 0; // Volta ao primeiro item\n      }\n\n      content.style.transform = `translateX(${position}px)`;\n    }\n\n    function autoSlide() {\n      slide('next');\n      if (position === 0) {\n        clearInterval(intervalId);\n        intervalId = setInterval(autoSlide, 5000);\n      }\n    }\n\n    nextButton.addEventListener('click', () => slide('next'));\n    prevButton.addEventListener('click', () => slide('prev'));\n\n    intervalId = setInterval(autoSlide, 5000);\n\n    items.forEach(item => {\n      item.addEventListener('click', () => {\n        const url = item.getAttribute('href');\n        window.open(url, '_blank');\n      });\n    });\n  </script>\n</body>\n</html>\n"
