      "html": "<body> \n  <h2>Veja avaliações de quem já comprou na Vitrine7shop!</h2>\n<div class=\"reviews-box\">\n  <div class=\"reviews-slider\">  \n    <div class=\"slide\">\n      <img src=\"img/uploads/vitrine7shop_mini-banner-reviews1.webp\">\n    </div>\n    <div class=\"slide\">\n      <img src=\"img/uploads/vitrine7shop_mini-banner-reviews2.webp\">\n    </div>\n    <div class=\"slide\">\n      <img src=\"img/uploads/vitrine7shop_mini-banner-reviews3.webp\">\n    </div>\n    <div class=\"slide\">\n      <img src=\"img/uploads/vitrine7shop_mini-banner-reviews4.webp\">\n    </div>\n    <div class=\"slide\">\n      <img src=\"img/uploads/vitrine7shop_mini-banner-reviews5.webp\">\n    </div>\n    <div class=\"slide\">\n      <img src=\"img/uploads/vitrine7shop_mini-banner-reviews6.webp\">\n    </div>\n    <div class=\"slide\">\n      <img src=\"img/uploads/vitrine7shop_mini-banner-reviews7.webp\">\n    </div>\n    <div class=\"slide\">\n      <img src=\"/img/uploads/vitrine7shop_mini-banner-reviews8.webp\">\n    </div>\n     <div class=\"slide\">\n      <img src=\"img/uploads/vitrine7shop_mini-banner-reviews9.webp\">\n    </div>\n       <div class=\"slide\">\n      <img src=\"img/uploads/vitrine7shop_mini-banner-reviews10.webp\">\n    </div> \n       <div class=\"slide\">\n      <img src=\"img/uploads/vitrine7shop_mini-banner-reviews11.webp\">\n    </div>\n       <div class=\"slide\">\n      <img src=\"img/uploads/vitrine7shop_mini-banner-reviews12.webp\">\n    </div>\n    <!-- Adicione mais slides aqui -->\n  </div>\n  <div class=\"slider-controls\">\n    <button class=\"prev-slide\">&lt;</button>\n    <button class=\"next-slide\">&gt;</button>\n  </div>\n</div>\n</body>\n\n<script>\n  const slider = document.querySelector('.reviews-slider');\n  const slides = slider.querySelectorAll('.slide');\n  const prevButton = document.querySelector('.prev-slide');\n  const nextButton = document.querySelector('.next-slide');\n  let currentIndex = 0;\n\n  function moveToSlide(slider, currentIndex, targetIndex) {\n    const currentSlide = slides[currentIndex];\n    const targetSlide = slides[targetIndex];\n    slider.style.transform = `translateX(-${targetSlide.offsetLeft}px)`;\n    currentSlide.classList.remove('active');\n    targetSlide.classList.add('active');\n  }\n\n  function updateButtons(currentIndex, prevButton, nextButton, slides) {\n    if (currentIndex === 0) {\n      prevButton.disabled = true;\n    } else {\n      prevButton.disabled = false;\n    }\n    if (currentIndex === slides.length - 1) {\n      nextButton.disabled = true;\n    } else {\n      nextButton.disabled = false;\n    }\n  }\n\n  function autoSlide() {\n    if (currentIndex === slides.length - 1) {\n      currentIndex = 0;\n    } else {\n      currentIndex++;\n    }\n    moveToSlide(slider, currentIndex - 1, currentIndex);\n    updateButtons(currentIndex, prevButton, nextButton, slides);\n  }\n\n  setInterval(() => {\n    autoSlide();\n  }, 10000);\n\n  prevButton.addEventListener('click', () => {\n    if (currentIndex > 0) {\n      currentIndex--;\n      moveToSlide(slider, currentIndex + 1, currentIndex);\n      updateButtons(currentIndex, prevButton, nextButton, slides);\n    }\n  });\n\n  nextButton.addEventListener('click', () => {\n    if (currentIndex < slides.length - 1) {\n      currentIndex++;\n      moveToSlide(slider, currentIndex - 1, currentIndex);\n      updateButtons(currentIndex, prevButton, nextButton, slides);\n    } else {\n      currentIndex = 0;\n      moveToSlide(slider, slides.length - 1, 0);\n      updateButtons(currentIndex, prevButton, nextButton, slides);\n    }\n  });\n\n  updateButtons(currentIndex, prevButton, nextButton, slides);\n  slides[currentIndex].classList.add('active');\n</script>\n"
